tolerance 2.0
filetype pdb
output Caffeine_water_1M_NaCl.pdb
add_box_sides 1.0

structure single-caffeine-molecule.pdb
	number 1
	fixed 0. 0. 0. 0. 0. 0.
        centerofmass
end structure

structure water.pdb
        number 2760
        inside cube 0. 0. 0. 45.
end structure

structure Na.pdb
        number 55
        inside cube 0. 0. 0. 45.
end structure

structure Cl.pdb
        number 55
        inside cube 0. 0. 0. 45.
end structure

